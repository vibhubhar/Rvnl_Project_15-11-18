		function editAssignEntity(SubUnitVal,SubUnitId,subUnitCompId ,obj) {
			var count =(obj.parentNode.parentNode.rowIndex)-1;
			 var $img = $("#assignImg"+count);
			var src = $img.attr("src")
			if(src == 'scripts/greenCirlce.jpg') {
				$("#shortModal1").modal('hide');
				return false;
			} 
			showModalMessage('');
			var crmComponentGroupId=document.getElementById("crmComponentGroup").value;
			var fmsProjectId=document.getElementById("fmsProject").value;
			var fmsPrjVal=$("#fmsProject option:selected").text();
			var crmComponentGroupVal=$("#crmComponentGroup option:selected").text();
			var tableData='';
			$.get("editAssignAjaxCompMeter", {
				SubUnitId : SubUnitId,
				subUnitCompId : subUnitCompId,
				crmComponentGroupId: crmComponentGroupId ,
				fmsProjectId: fmsProjectId,
				eParam  : "search"
			}, function(data) {
				var list = JSON.parse(data);
				$(":input[name='eProject']").val(fmsPrjVal);
				$(":input[name='eSubUnit']").val(SubUnitVal);
				$(":input[name='eSubUnitHidden1']").val(SubUnitId);
				$(":input[name='eComponent']").val(crmComponentGroupVal);
				$(":input[name='eComponentHidden']").val(crmComponentGroupId);
				if (list.length > 0) {
					var i;
					for( i=0;i<list.length;i++){
						tableData=tableData+
						'<tr style="font-size: 200px;">' +
						'<td><input type="hidden" id="eSubUnitCompMeterId' + (i) + '" name="eSubUnitCompMeterId"  value="'+list[i].eSubUnitCompMeterId+'" />'  
						 +'<input type="hidden" id="eSubUnitCompId' + (i) + '" name="eSubUnitCompId"  value="'+list[i].eSubUnitCompId+'" /><input type="hidden" id="eMeterHiddId' + (i) + '" name="eMeterHiddId"  value="'+list[i].eMeterHiddId+'" /> <input id="eMeterNum'+(i)+'" name="eMeterNum" type="text" style="width:140px;" class="form-control" readonly="true" value="'+list[i].eMeterNum+'"  /></td>' +
						 '<td> <input type="text" id="eStartDate' + (i) + '"  style="width:100px;"  name="eStartDate" readonly="true"  value="'+list[i].eStartDate+'" class="form-control"   /></td>' ;
						if(list[i].eEndDate !=''){
							tableData=tableData+ '<td><input type="text" id="eEndDate' + (i) + '"   style="width:100px;" name="eEndDate" value="'+list[i].eEndDate+'" class="form-control" placeholder="dd-Mon-yyyy" readonly="true"/></td>' ;	
						}else{
							tableData=tableData+ '<td><input type="text" id="eEndDate' + (i) + '"    style="width:100px;" name="eEndDate" value="'+list[i].eEndDate+'" class="form-control from" placeholder="dd-Mon-yyyy" /></td>' ;
						}
						tableData=tableData+'<td><input type="checkbox" id="eActive' + (i) + '"   name="eActive"  class="form-control" '+
						list[i].eActive  
						+' style="padding: 0px; height: 0px;margin-left: 15px; "/></td>' 
						+'<td><input type="hidden" id="eMeterTypeId' + (i) + '" name="eMeterTypeId"  value="'+list[i].eMeterTypeId+'" /><input type="text" id="eMeterType' + (i) + '"   name="eMeterType" readonly="true" value="'+list[i].eMeterType+'" class="form-control"  /></td>' +
						'<td><input type="hidden" id="eConnTypeId' + (i) + '" name="eConnTypeId"  value="'+list[i].eConnTypeId+'" /><input type="text" id="eConnType' + (i) + '"   name="eConnType" readonly="true" value="'+list[i].eConnType+'" class="form-control" /></td>' +
						'<td><input type="text" id="eOldReading' + (i) + '"   name="eOldReading" readonly="true" value="'+list[i].eOldReading+'" class="form-control" /></td>' +
						'<td><input type="text" id="eOldMeter' + (i) + '"   name="eOldMeter"  value="'+list[i].eOldMeter+'" readonly="readonly" class="form-control" /><input type="hidden" id="eOldMeterId' + (i) + '"   name="eOldMeterId" value="'+list[i].eOldMeterId+'"  /></td>' +
						'<td><input type="text" id="eMessageId' + (i) + '"   name="eMessageId"   readonly="readonly" class="form-control" /></td>' +
						+' </tr>';
						}
		}
		$("#ModCompMeterBody").html('');
		$("#ModCompMeterBody").append(tableData);
		$("#shortModal1").modal('show');
		$('.from').datepicker({
	        numberOfMonths: 1,
	        autoclose: true,
	        changeMonth: true,
	        changeYear: true,
	        dateFormat:$('#systemDatePickerFormat').val(),
	        onSelect: function (selected) {
	        	   var dt = new Date(selected);
	            dt.setDate(dt.getDate() + 1);            
	            DateOverlapCheck(this);
	        }
	    });
		});
		}
		
		 function searchMeterModal() {
			var eRowIndex= $("#eRowIndex").val();
			 var eSubUnitCompHdr=$("#eSubUnitCompHdr").val();
			 var eCompGrp=$("#eCompGrp").val();
			var eSearch=$("#eSearch").val();
			var tableData='';
			$.get("openMeterModal", {
				eSubUnitCompHdr : eSubUnitCompHdr,
				eCompGrp : eCompGrp,
				eSearch: eSearch,
				eParam  : "search"
			}, function(data) {
			var list = JSON.parse(data);
			if (list.length > 0) {
				var i;
				for( i=0;i<list.length;i++){
					tableData=tableData+'<tr ><td><input type="radio" id="eSelect' +(i)+ '" name="eSelect"  onclick="selectVal(' +(i)+ ')" /></td>' 
					+'<td><input type="hidden" id="eMeterN0' + (i) + '" name="eMeterN0"  value="'+list[i].eMeterNum+'" /><input type="hidden" id="eMeterHidd' + (i) + '" name="eMeterHidd"  value="'+list[i].eMeterHidd+'" />'+list[i].eMeterNum+'</td>'
					+'</tr>';
					}
	}
	$("#ModMeterNumBody").html('');
	$("#ModMeterNumBody").append(tableData);
	$("#shortModal2").modal('show');
	});
	}
	 function selectVal(rowIndex){
		 var eRowIndex= $("#eRowIndex").val();
		document.getElementById("sMeterNumHidden"+eRowIndex).value=$("#eMeterHidd"+rowIndex).val();
		document.getElementById("sMeterNum"+eRowIndex).value=$('#eMeterN0'+rowIndex).val();
		$("#shortModal2").modal('hide');
		} 
		function callpopup1(row_index) {
			$(":input[name='eRowIndex']").val(row_index);
			$(":input[name='eCompGrp']").val($("#crmComponentGroup").val());
			$(":input[name='eSubUnitCompHdr']").val($("#sSubUnitCompId1"+row_index).val());
			checkCheckbox(row_index);
			$("#ModMeterNumBody").html('');
			$("#shortModal2").modal('show');
			
		}
	function DateOverlapCheck(elementDate)
	{
			showModalMessage('');
			var tr =elementDate.parentNode.parentNode;
			var dataLen=tr.parentNode.getElementsByTagName("tr").length;
			var index=tr.rowIndex;
			var rowIndex= index-1;
		    var startDate= document.getElementsByName("eStartDate");
		 	var endDate = document.getElementsByName("eEndDate");
			var selectedstartDate= startDate[index-1].value;
			var selectedEndDate= endDate[index-1].value;
			if(selectedstartDate=='')
			{
				showModalMessage("Please Select the start date @row "+index);
				return false;
			}
			for(var i=0;i<dataLen;i++)
			{
				var startDate= document.getElementsByName("eStartDate")[i];
				var endDate= document.getElementsByName("eEndDate")[i];
				if(startDate.value=='')
				{
					showModalMessage("Please enter start date @row ."+rowIndex);
				    return false;
				}
				if(rowIndex!=i&&dateCheck(startDate.value,endDate.value,selectedstartDate))
				{
					
					showModalMessage("Start Date Selected cannot lie between previous selected dates  @row "+(rowIndex+1));
				return false;
				}
				if(rowIndex!=i&&dateCheck(selectedstartDate,selectedEndDate,startDate.value))
				{
					showModalMessage("Date Overlapping :Start Date @ row "+ (i+1)+" lies between selected dates  @row "+(rowIndex+1));
					return false;
				}
				if(selectedEndDate!='')
				{
				    if(rowIndex!=i&&dateCheck(startDate.value,endDate.value,selectedEndDate))
					{
				    	showModalMessage("End Date Selected cannot lie between previous selected dates   @row "+(rowIndex+1));
						return false;
					}
				    if(rowIndex!=i&&dateCheck(selectedstartDate,selectedEndDate,endDate.value))
					{
						
						showModalMessage("Date Overlapping :End Date @ row "+ (i+1)+" lies between selected dates  @row "+(rowIndex+1));
						return false;
					}
				}
				if(endDate.value!=''){
					if(parseDate(endDate.value)<parseDate(startDate.value)){
						showModalMessage("Start Date cannot be greater than End date @row "+(rowIndex+1));
						return false;
					}
				}
				
			}	
			return true;
	}


	 
	 function dateCheck(from,to,check){
		var fDate,lDate,cDate;
	    fDate = parseDate(from);
	    var newdate = new Date();
	    if(to!=''){
	    lDate = parseDate(to);
	    }
	    else{
	     lDate=newdate.setDate(newdate.getDate()+10000);
	      var dd = newdate.getDate();
	      var mm = newdate.getMonth() + 1;
	      var y = newdate.getFullYear(); 
	    lDate= new Date(y,mm,dd);
	   }
	  	cDate = parseDate(check);

	    if(cDate<=lDate && cDate>= fDate)
	    	{
	    	if(cDate<=lDate && cDate>= fDate){
	    		return true;
	    	}
	        	
	    	}
	    
    	
	    return false;
	} 

	 function parseDate(s){
		  var months = {jan:0,feb:1,mar:2,apr:3,may:4,jun:5,
		                jul:6,aug:7,sep:8,oct:9,nov:10,dec:11};
		  var p = s.toString().split('-');
		  return new Date(p[2], months[p[1].toLowerCase()], p[0]);
	} 
	function saveAssignBtn(){
		if($("#modalMessageId").hasClass("error")){
			return false;
		}
		var sSubUnitCompMeterIdStr='';
		var eSubUnitCompIdStr='';
		var eMeterHiddIdStr='';
		var eStartDateStr='';
		var eEndDateStr='';
		var eActiveStr='';
		var eMeterTypeIdStr='';
		var eConnTypeIdStr='';
		var eOldReadingStr='';
		var eOldMeterIdStr='';
		
		
		
		var eSubUnitCompMeterId=document.getElementsByName("eSubUnitCompMeterId");
		var eSubUnitCompId=document.getElementsByName("eSubUnitCompId");
		var eMeterHiddId=document.getElementsByName("eMeterHiddId");
		var eStartDate=document.getElementsByName("eStartDate");
		var eEndDate=document.getElementsByName("eEndDate");
		var eActive =document.getElementsByName("eActive");
		var eMeterTypeId =document.getElementsByName("eMeterTypeId");
		var eConnTypeId =document.getElementsByName("eConnTypeId");
		var eOldReading =document.getElementsByName("eOldReading");
		var eOldMeterId =document.getElementsByName("eOldMeterId");
		
		
		var len =eSubUnitCompMeterId.length;
		for(var i=0;i<len;i++ ){
				sSubUnitCompMeterIdStr=sSubUnitCompMeterIdStr.concat(eSubUnitCompMeterId[i].value + "^");
				eSubUnitCompIdStr=eSubUnitCompIdStr.concat(eSubUnitCompId[i].value+"^");
				eMeterHiddIdStr=eMeterHiddIdStr.concat(eMeterHiddId[i].value+"^");
				eStartDateStr=eStartDateStr.concat(eStartDate[i].value+"^");
				eEndDateStr=eEndDateStr.concat(eEndDate[i].value+"^");
				if(eActive[i].checked){
					eActiveStr=eActiveStr.concat("1"+"^");
				}else{
				eActiveStr=eActiveStr.concat("0"+"^");
				}
				eMeterTypeIdStr=eMeterTypeIdStr.concat(eMeterTypeId[i].value + "^");
				eConnTypeIdStr=eConnTypeIdStr.concat(eConnTypeId[i].value + "^");
				eOldReadingStr=eOldReadingStr.concat(eOldReading[i].value + "^");
				eOldMeterIdStr=eOldMeterIdStr.concat(eOldMeterId[i].value + "^");
		} 
		$.get("addAjaxUnitCompMeter", {
			sSubUnitCompMeterId :sSubUnitCompMeterIdStr ,
			sSubUnitCompId1 :eSubUnitCompIdStr ,
			sMeterNumHidden : eMeterHiddIdStr,
			sFrom : eStartDateStr,
			sTo :eEndDateStr ,
			sActive :eActiveStr ,
			sMeterType: eMeterTypeIdStr,
			sConnType :eConnTypeIdStr ,
			sOldReading :eOldReadingStr,
			sOldMeterId :eOldMeterIdStr,
			eParam: "save"
				
		}, function(data) {
			 var list = JSON.parse(data);
			 var errorCode=list[0].errorCode;
			 var errorCodeArr = new Array(); 
		     errorCodeArr = errorCode.split("^");
		     var j = 0;		 
				if (errorCodeArr.length != '' ) {
					for (var i = 0; i < len; i++) {
							 document.getElementById("eMessageId"+ i).value = errorCodeArr[j];
							  j++;
						}
					}
				
			});
	}
