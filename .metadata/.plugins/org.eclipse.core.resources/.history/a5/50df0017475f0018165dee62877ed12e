function mailschedular(){
	
	 var option = document.getElementById("mail").value;
	 if(option=="off"){
		 jQuery.noConflict();
			
		 var dialogid="#dialogid1";
		 
		 $(dialogid).dialog({
	        modal: true,
	        autoOpen: false,
	        title: "Alert",
	        width: 400,
	        height: 200
	    });
		 
		  $(dialogid).dialog('open');
          return false;
	 }
	 else{
		 jQuery.noConflict();
			
		 var dialogid="#dialogid";
		 
		 $(dialogid).dialog({
	        modal: true,
	        autoOpen: false,
	        title: "Alert",
	        width: 400,
	        height: 200
	    });
		 
		  $(dialogid).dialog('open');
          return false;
	 }
}

